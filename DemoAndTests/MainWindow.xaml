<Window x:Class="DemoAndTests.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoAndTests"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
  <Grid>
    <Button Content="OpenWindow" HorizontalAlignment="Left" Margin="149,73,0,0" VerticalAlignment="Top" Width="111" Click="Button_Click_1"/>
    <Button Content="UpdateText" HorizontalAlignment="Left" Margin="273,229,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    <Label Content="{Binding TestData}" HorizontalAlignment="Left" Margin="403,132,0,0" VerticalAlignment="Top" Width="295"/>
    <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="433,94,0,0" VerticalAlignment="Top" IsChecked="{Binding Status, Mode=TwoWay}"/>
    <ComboBox HorizontalAlignment="Left" Margin="433,132,0,0" VerticalAlignment="Top" Width="120"/>
    <TextBox HorizontalAlignment="Left" Height="24" Margin="433,180,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

  </Grid>
</Window>
