<Window x:Class="IViewer.OtherWindow.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IViewer.OtherWindow"
        xmlns:iViewer="clr-namespace:IViewer"
        mc:Ignorable="d"
        Title="ConfigWindow" Height="300" Width="400" Background="#FF121212" Closed="Window_Closed">
    <Window.DataContext>
        <iViewer:TomlWatcher></iViewer:TomlWatcher>
    </Window.DataContext>
    <Grid>
        <TextBox x:Name="LongBox" HorizontalAlignment="Left" Height="22" Margin="68,44,0,0" TextWrapping="Wrap" Text="{Binding LongResult}" VerticalAlignment="Top" Width="264"/>
        <TextBox x:Name="BoolBox" HorizontalAlignment="Left" Height="22" Margin="68,96,0,0" TextWrapping="Wrap" Text="{Binding BoolResult}" VerticalAlignment="Top" Width="264"/>
        <TextBox x:Name="StringBox" HorizontalAlignment="Left" Height="22" Margin="68,148,0,0" TextWrapping="Wrap" Text="{Binding StringResult}" VerticalAlignment="Top" Width="264"/>
        <TextBox x:Name="DoubleBox" HorizontalAlignment="Left" Height="22" Margin="68,200,0,0" TextWrapping="Wrap" Text="{Binding DoubleResult}" VerticalAlignment="Top" Width="264"/>

    </Grid>
</Window>
